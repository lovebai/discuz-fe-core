{
  "presets": [
    [
      "@babel/env",
      {
        "targets": { "browsers": ["> 1%", "last 2 versions", "not ie <= 9"] },
        "useBuiltIns": "usage",
        "corejs": "3"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-transform-runtime"
  ]
}